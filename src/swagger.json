{
    "swagger": "2.0",
    "info": {
        "title": "Mi API de peliculas tipo Neflis",
        "version": "1.0.0"
    },
    "paths": {
        "/api/v1/users": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Obtener todos mis usuarios"
                    },
                    "400" : {
                        "description": "Ocurrio un error al intentar obtener todos los usuarios"
                    }
                }
            }
        },
        "/api/v1/movies": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Obtener todos mis peliculas"
                    },
                    "400" : {
                        "description": "Ocurrio un error al intentar obtener todos las peliculas"
                    }
                }
            }
        },
        "/api/v1/movies/{movieId}": {
            "get": {
                "parameters": [
                    {
                        "name": "movieId",
                        "in": "path",
                        "type": "string",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Obtener todos mis peliculas"
                    },
                    "400" : {
                        "description": "Ocurrio un error al intentar obtener todos las peliculas"
                    }
                }
            }
        }
    }
}